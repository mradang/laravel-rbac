{
    "name": "mradang/laravel-rbac",
    "description": "laravel-rbac",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "mradang",
            "email": "mradang@163.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "firebase/php-jwt": "^5.0",
        "laravel/framework": "~6.0||~7.0||~8.0",
        "overtrue/laravel-pinyin": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "mradang\\LaravelRbac\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mradang\\LaravelRbac\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mradang\\LaravelRbac\\LaravelRbacServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^6.18",
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "^4.7"
    }
}
